

{
  
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {

        "src": "/(.*)",
   
        "dest": "index.js"

      }
    ],

    "redirects": [
      { "source": "/employeesPage", "destination": "/employeespage.ejs", "permanent": false }
      
    ]
  }

